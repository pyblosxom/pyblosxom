<?xml version="1.0" encoding="$blog_encoding"?>

<feed xmlns="http://www.w3.org/2005/Atom" xml:lang="$(blog_language)">
<title type="text">$(escape(blog_title))</title>
<subtitle type="html">
$(escape(blog_description))
</subtitle>
<id>$(url)</id>
<link rel="alternate" type="text/html" href="$(base_url)" />
<link rel="self" type="application/atom+xml" href="$(url)" />


<author>
<name>$(escape(blog_author))</name>
<uri>$(escape(url))</uri>
<email>$(escape(blog_email))</email>
</author>
<rights>$(escape(blog_rights))</rights>
<generator uri="http://pyblosxom.bluesock.org/" version="$(escape(pyblosxom_version))">
Pyblosxom http://pyblosxom.bluesock.org/ $(escape(pyblosxom_version))
</generator>

<updated>$(latest_w3cdate)</updated>
